{{if rows:}}
<table class="table">
    <thead>
        <tr>
            {{for h in headers:}}
            <th>{{=h}}</th>
            {{pass}}
        </tr>
    </thead>
    <tbody>
        {{for row in rows:}}
        <tr>
            {{for cell in row:}}
            <td>{{=cell}}</td>
            {{pass}}
        </tr>
        {{pass}}
    </tbody>
</table>

{{else:}}
No switches found for this transmitter.
{{pass}}